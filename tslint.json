{
    "extends": "tslint:latest",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            false
        ],
        "member-ordering": [
            true,
            {
                "order": "fields-first"
            }
        ],
        "object-literal-sort-keys": false,
        "radix": false,
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "max-classes-per-file": [
            false
        ],
        "arrow-parens": false,
        "no-string-literal": false,
        "only-arrow-functions": [
            false
        ]
    }
}